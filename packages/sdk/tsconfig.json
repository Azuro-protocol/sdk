{
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["es2021", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2021",
    "types": ["node"],
    "paths": {

    }
  },
  "exclude": [
    "**/node_modules/**",
    "**/dist/**"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
